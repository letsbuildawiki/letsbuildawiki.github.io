---
pagination:
  data: repos
  size: 500
---
<h1>Index</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
    {%- for item in pagination.items %}
      <tr>
        <td>{{ item.name }}</td>
        <td>{{ item.url }}</td>
      </tr>
    {% endfor -%}
  </tbody>
</table>
